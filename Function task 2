import random
guess = 0
comp_num = 0
def pickNums():
    global comp_num
    low = int(input("Enter a low number"))
    high = int(input("Enter a high number"))
    comp_num = random.randint(low, high)

def guessNum():
    global guess
    print("I am thinking of a number.")
    guess = int(input("Guess the number: "))
def checkNums():
    global comp_num, guess
    if guess == comp_num:
        print("Correct, you win")
    else:
        while comp_num != guess:
            print("Incorrect.")
            if comp_num<guess:
                guess = int(input("Your guess was too high. Guess again: "))
            elif comp_num>guess:
                guess = int(input("Your guess was too low. Guess again: "))
        print("Correct, you win")
            


pickNums()
guessNum()
checkNums()
